<beans xmlns="http://www.springframework.org/schema/beans"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xsi:schemaLocation="http://www.springframework.org/schema/beans       http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">    <bean id="shiroFilter" class="org.apache.shiro.spring.web.ShiroFilterFactoryBean">        <property name="securityManager" ref="securityManager"/>        <property name="loginUrl" value="/signin"/>        <property name="successUrl" value="/"/>        <property name="filterChainDefinitions">            <value>                / = anon                /signin = authc                /signup = anon                /app_asserts/** = anon                /oz/webui/** = anon                /**/*.js = anon                /**/*.css = anon                /asserts/** = anon                /validateCode = anon                /v1/** = noSessionCreation,anon                /login = authc                /logout = logout                /** = user            </value>        </property>    </bean></beans>